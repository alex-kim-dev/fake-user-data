{
  "name": "fake-user-data",
  "version": "1.0.0-alpha.0",
  "private": true,
  "description": "A client-server web app that generates user data with mistakes based on a given seed",
  "homepage": "https://github.com/alex-kim-dev/fake-user-data#readme",
  "bugs": "https://github.com/alex-kim-dev/fake-user-data/issues",
  "repository": "git@github.com:alex-kim-dev/fake-user-data.git",
  "license": "MIT",
  "author": "Alex Kim (https://github.com/alex-kim-dev)",
  "type": "module",
  "scripts": {
    "build": "conc npm:build:*",
    "build:client": "rimraf dist/client && vite build --outDir dist/client",
    "build:server": "rimraf dist/server && esbuild server/app.ts --bundle --platform=node --outfile=dist/server/index.cjs",
    "format": "prettier --check \"**/*.{html,json,yml,md}\"",
    "lint:code": "eslint --cache --ignore-path .gitignore --ext .ts,.tsx .",
    "lint:styles": "stylelint --cache --ignore-path .gitignore \"client/**/*.scss\"",
    "prepare": "husky",
    "serve": "conc npm:serve:*",
    "serve:client": "vite preview --outDir dist/client",
    "serve:server": "node dist/server/index.cjs",
    "start": "conc npm:start:*",
    "start:client": "vite",
    "start:server": "tsx --watch server/app.ts",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@faker-js/faker": "8.4.1",
    "axios": "1.6.8",
    "bootstrap": "5.3.3",
    "clsx": "2.1.1",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "export-to-csv": "1.3.0",
    "express": "4.19.2",
    "react": "18.2.0",
    "react-bootstrap-icons": "1.11.4",
    "react-dom": "18.2.0",
    "react-intersection-observer": "9.10.1",
    "seedrandom": "3.0.5",
    "underscore": "1.13.6"
  },
  "devDependencies": {
    "@commitlint/cli": "19.2.1",
    "@commitlint/config-conventional": "19.1.0",
    "@types/express": "4.17.21",
    "@types/node": "20.12.7",
    "@types/react": "18.2.73",
    "@types/react-dom": "18.2.23",
    "@types/seedrandom": "3.0.8",
    "@types/underscore": "1.11.15",
    "@typescript-eslint/eslint-plugin": "7.5.0",
    "@typescript-eslint/parser": "7.5.0",
    "@vitejs/plugin-react-swc": "3.6.0",
    "concurrently": "8.2.2",
    "esbuild": "0.20.2",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "12.0.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "prettier-plugin-packagejson": "2.4.14",
    "sass": "1.75.0",
    "stylelint": "16.3.1",
    "stylelint-config-recess-order": "5.0.0",
    "stylelint-config-standard-scss": "13.1.0",
    "stylelint-order": "6.0.4",
    "stylelint-prettier": "5.0.0",
    "tsc-files": "1.1.4",
    "tsx": "4.7.2",
    "typescript": "5.4.3",
    "vite": "5.2.7",
    "vite-plugin-svgr": "4.2.0",
    "vite-tsconfig-paths": "4.3.2"
  }
}
